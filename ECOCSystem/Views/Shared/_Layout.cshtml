            display: flex;
            justify-content: center;
        }

        .navbar-light .navbar-nav .nav-link {
            color: white;
        }

        [class*=sidebar-dark-] .sidebar a {
            color: #f8f9fa;
        }

        .navbar-white {
     /*         background-color: #1977cc;*/
            background-color: #006400e0;
            color: #f8f9fa;
            /* background-image: linear-gradient(45deg, #343a40, #000000f5);*/
    /*              background-image: linear-gradient(45deg, #343a408c, #000000f5);*/
            margin-bottom: 7px;
        }

        .navbar-light .navbar-nav .nav-link:focus, .navbar-light .navbar-nav .nav-link:hover {
            color: white;
        }


        /* ADD FLOATING BUTTON */
        div.fab {
            z-index: 1;
            width: 56px;
            height: 56px;
            background-color: green;
            border-radius: 50%;
            box-shadow: 0 6px 10px 0 #666;
            font-size: 28px;
            font-weight: 800;
            line-height: 56px;
            color: white;
            text-align: center;
            position: fixed;
            right: 25px;
            bottom: 25px;
            transition: all 0.1s ease-in-out;
        }

            div.fab:hover {
                box-shadow: 0 6px 14px 0 #666;
                transform: scale(1.05);
            }


        /*Hide label top on data tables*/
        div.dataTables_wrapper div.dataTables_length label {
            display: none;
        }


        .modal-content {
            padding: 11px;
        }

        .form-control {
            border: 0;
            border-bottom: 1px solid #dee2e6;
            border-radius: 0;
        /*            color:black;*/
        }
        .select2-container--bootstrap4 .select2-selection--single .select2-selection__rendered {

            color: black;
        }

        .field-validation-error {
            color: indianred;
        }



        .submit-btn {
            display: flex;
            align-items: center;
            justify-content: center;
        }

            .submit-btn button {
                width: 300px;
            }


        .btn-info {
            color: #fff;
            background-color: #117a8b;
            border-color: #117a8b;
        }

        .btn-success {
            color: #fff;
            background-color: #218838;
            border-color: #218838;
        }


        .select2-container--bootstrap4 .select2-selection--single {
            border: 0;
            border-bottom: 1px solid #dee2e6;
            border-radius: 0;
        }





        .loading {
            position: fixed;
            z-index: 99999;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            background-color: rgba(0,0,0,.7);
        }

            .loading .circle {
                width: 30px;
                height: 30px;
                border-radius: 50%;
                margin-bottom: 10px;
                animation: blink 1s infinite;
            }

            .loading .red {
                background-color: #117a8b;
            }

            .loading .yellow {
                background-color: #117a8b;
                animation-delay: 0.33s;
            }

            .loading .green {
                background-color: #117a8b;
                animation-delay: 0.66s;
            }

        @@keyframes blink {
            0% {
                opacity: 0;
            }

            50% {
                opacity: 1;
            }

            100% {
                opacity: 0;
            }
        }

        .btn-success {
            background-color: #006400e0;
            border-color: #006400e0;
        }


        [class*=sidebar-light] .brand-link, [class*=sidebar-light] .brand-link .pushmenu {
            color: white;
            background-color: #006400e0;
            /* background-image: linear-gradient(45deg, #343a408c, #000000f5); */
        }

        [class*=sidebar-light-] .nav-sidebar > .nav-item.menu-open > .nav-link, [class*=sidebar-light-] .nav-sidebar > .nav-item:hover > .nav-link {
            background-color: green;
            color: white;
        }
        [class*=sidebar-light-] .sidebar a {
            color: #000000d6;
        }

        [class*=sidebar-light-] {
            background-color: #ececec;
        }
    </style>
</head>
